/usr/bin/moc:
	(cd "$(QTDIR)/src/tools/moc" && $(MAKE))

mocclean: compiler_moc_header_clean compiler_moc_source_clean

mocables: compiler_moc_header_make_all compiler_moc_source_make_all

compiler_moc_header_make_all: moc_client.cpp moc_MainWindow.cpp
compiler_moc_header_clean:
	-$(DEL_FILE) moc_client.cpp moc_MainWindow.cpp
moc_client.cpp: CanPacket.h \
		client.h \
		/usr/bin/moc-qt4
	/usr/bin/moc $(DEFINES) $(INCPATH) client.h -o moc_client.cpp

moc_MainWindow.cpp: ui_mainwindow.h \
		MainWindow.h \
		/usr/bin/moc-qt4
	/usr/bin/moc $(DEFINES) $(INCPATH) MainWindow.h -o moc_MainWindow.cpp


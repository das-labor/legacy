#!/bin/sh

rrdtool create powermeter.rrd \
	-s 1 \
	DS:U1:GAUGE:10:50:500     \
	DS:U2:GAUGE:10:50:500     \
	DS:U3:GAUGE:10:50:500     \
	DS:I1:GAUGE:10:0:50       \
	DS:I2:GAUGE:10:0:50       \
	DS:I3:GAUGE:10:0:50       \
	DS:S1:GAUGE:10:0:6000     \
	DS:S2:GAUGE:10:0:6000     \
	DS:S3:GAUGE:10:0:6000     \
	DS:P1:GAUGE:10:0:6000     \
	DS:P2:GAUGE:10:0:6000     \
	DS:P3:GAUGE:10:0:6000     \
	RRA:AVERAGE:0.5:2:43200   \
	RRA:AVERAGE:0.5:60:14400  \
	RRA:AVERAGE:0.5:3600:8760 \
	RRA:AVERAGE:0.0:86400:3560 \
	RRA:MAX:0.5:2:43200   \
	RRA:MAX:0.5:60:14400  \
	RRA:MAX:0.5:3600:8760 \
	RRA:MAX:0.0:86400:3560 \
	

#
# This file contains the instruction set and parameter data
# used to generate a AVR assembler and disassembler
#
#

parameters{
  R = r[0..31];
  RL = r[0..15];
  RH = r[16..31];
  RW = r24,r26,r28,r30;
  RM = r[16..23];
  RPTR = X,Y,Z;
  K = [0..63];
  Byte = [0..255];
}  


cpu{
  flags=C,Z,N,V,S,H,T,I;
  registers=r[0..31],X,Y,Z;
}

instructionset{
  ADD

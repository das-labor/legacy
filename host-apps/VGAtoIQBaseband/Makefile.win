LINKOBJ  = glInfo.o main.o Timer.o $(RES)
LIBS =  -L"D:/song/Dev-Cpp/lib" -L"D:/song/MinGW/lib" -lglut32 -lglu32 -lopengl32 -lwinmm -lgdi32  
INCS =  -I"D:/song/Dev-Cpp/include"  -I"D:/song/MinGW/include" 

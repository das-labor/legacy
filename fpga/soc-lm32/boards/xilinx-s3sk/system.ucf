# ==== Clock inputs (CLK) ====
NET "clk" LOC = "T9" | IOSTANDARD = LVCMOS33 ;
NET "clk" PERIOD = 20 HIGH 50%;

# ==== UART ====
NET "uart_rxd" LOC = "T13" | IOSTANDARD = LVTTL ;
NET "uart_txd" LOC = "R13" | IOSTANDARD = LVTTL | DRIVE = 8 | SLEW = SLOW ;


    bool 'Enable console UART                  ' CONFIG_UART1_ENABLE
    if [ "$CONFIG_UART1_ENABLE" = "y" ]; then
        choice 'UART1 FIFO depth' \
	  "1		CONFIG_UA1_FIFO1 \
	   2		CONFIG_UA1_FIFO2 \
	   4		CONFIG_UA1_FIFO4 \
	   8		CONFIG_UA1_FIFO8 \
	  16		CONFIG_UA1_FIFO16 \
	  32  		CONFIG_UA1_FIFO32" 1
    fi

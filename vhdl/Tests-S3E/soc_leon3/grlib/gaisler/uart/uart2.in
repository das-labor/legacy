    bool 'Enable secondary UART                ' CONFIG_UART2_ENABLE
    if [ "$CONFIG_UART2_ENABLE" = "y" ]; then
        choice 'UART2 FIFO depth' \
	  "1		CONFIG_UA2_FIFO1 \
	   2		CONFIG_UA2_FIFO2 \
	   4		CONFIG_UA2_FIFO4 \
	   8		CONFIG_UA2_FIFO8 \
	  16		CONFIG_UA2_FIFO16 \
	  32  		CONFIG_UA2_FIFO32" 1
    fi
